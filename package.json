{
  "name": "rnappium",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --ci --runInBand --reporters=default --reporters=jest-junit -t '$TEST'",
    "test:build:android": "bash scripts/build-android.sh",
    "test:build:ios": "bash scripts/build-ios.sh",
    "test:android": "wdio wdio.conf.android.js",
    "test:ios": "wdio wdio.conf.ios.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-testid": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@wdio/appium-service": "^7.7.3",
    "@wdio/cli": "^7.7.4",
    "@wdio/local-runner": "^7.7.4",
    "@wdio/mocha-framework": "^7.7.4",
    "@wdio/spec-reporter": "^7.7.3",
    "@wdio/sync": "^7.7.4",
    "appium": "^1.21.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "jest-junit": "^4.0.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "ts-jest": "23.10.5"
  },
  "jest": {
    "addFileAttribute": "true",
    "verbose": true,
    "testResultsProcessor": "jest-junit",
    "modulePaths": [
      "<rootDir>"
    ],
    "preset": "react-native",
    "rootDir": "src"
  },
  "jest-junit": {
    "suiteName": "Test Suite",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "usePathForSuiteName": "true"
  }
}
